<Defs>
  <ThingDef Name="ImpregnaWall" ParentName="BuildingBase">
    <defName>ImpregnaWall</defName>
    <label>ImpregnaWall</label>
    <thingClass>Building</thingClass>
    <category>Building</category>
    <description>The ImpregnaWall! If death finds you behind these walls, it came from within.</description>
    <graphicData>
      <texPath>Things/Building/Linked/Wall</texPath>
      <graphicClass>Graphic_Appearances</graphicClass>
      <linkType>CornerFiller</linkType>
